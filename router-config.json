{
	"updaters": [
		{
			"type": "stop-time-updater",
			"sourceType": "gtfs-http",
			"frequencySec": 60,
			"feedId": "${GTFS_FEED_ID}",
			"url": "${GTFS_RT_URL}"
		},
		{
			"type": "bike-rental",
			"frequencySec": 120,
			"sourceType": "gbfs",
			"network": "BZ",
			"url": "${GBFS_HOST}/bz/${GBFS_VERSION}/"
		},
		{
			"type": "bike-rental",
			"frequencySec": 120,
			"sourceType": "gbfs",
			"network": "ME",
			"url": "${GBFS_HOST}/me/${GBFS_VERSION}/"
		},
		{
			"type": "bike-rental",
			"frequencySec": 120,
			"sourceType": "gbfs",
			"network": "PAPIN",
			"url": "${GBFS_HOST}/papin/${GBFS_VERSION}/"
		},
		{
		    "type": "car-rental-updater",
		    "sourceType": "car2go",
		    "frequencySec": 30,
		    "vehiclesUrl": "${CARSHARING_HOST}/carsharing/vehicles.json",
		    "regionsUrl": "${CARSHARING_HOST}/carsharing/regions.json"
		},
{
			"type": "car-park",
			"sourceType": "park-and-ride",
			"frequencySec": 60,
			"url": "${PARKING_HOST}/parking/park-ride.json"
		}
	]
}
