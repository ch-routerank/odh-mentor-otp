			"url": "${GBFS_HOST}:${GBFS_PORT}/bz/${GBFS_VERSION}/"
		},
		{
			"type": "bike-rental",
			"frequencySec": 900,
			"sourceType": "gbfs",
			"network": "ME",
			"url": "${GBFS_HOST}:${GBFS_PORT}/me/${GBFS_VERSION}/"
		},
		{
			"type": "bike-rental",
			"frequencySec": 900,
			"sourceType": "gbfs",
			"network": "PAPIN",
			"url": "${GBFS_HOST}:${GBFS_PORT}/papin/${GBFS_VERSION}/"
		},
		{
		    "type": "car-rental-updater",
		    "sourceType": "car2go",
		    "frequencySec": 10,
		    "vehiclesUrl": "http://carsharing:8091/carsharing/vehicles.json",
